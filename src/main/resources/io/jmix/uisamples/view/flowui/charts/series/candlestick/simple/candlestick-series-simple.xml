<view xmlns="http://jmix.io/schema/flowui/view"
      xmlns:charts="http://jmix.io/schema/charts/ui">
    <layout>
        <charts:chart id="chart" width="100%" height="100%" minHeight="30em">
            <charts:xAxes>
                <charts:xAxis/>
            </charts:xAxes>

            <charts:yAxes>
                <charts:yAxis/>
            </charts:yAxes>

            <charts:series>
                <charts:candlestick/>
            </charts:series>
        </charts:chart>
    </layout>
</view>
